<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
	<filter>
		<filter-name>EncodingFilter</filter-name>
		<filter-class>com.android.filter.EncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
	</filter>

	<filter-mapping>
		<filter-name>EncodingFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	
  <servlet>
    <servlet-name>UserJsonServlet</servlet-name>
    <servlet-class>com.android.servlet.UserJsonServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginJsonServlet</servlet-name>
    <servlet-class>com.android.servlet.LoginJsonServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FacilityJsonServlet</servlet-name>
    <servlet-class>com.android.servlet.FacilityJsonServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PropertyJsonServlet</servlet-name>
    <servlet-class>com.android.servlet.PropertyJsonServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CityJsonServlet</servlet-name>
    <servlet-class>com.android.servlet.CityJsonServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UploadJsonServlet</servlet-name>
    <servlet-class>com.android.servlet.UploadJsonServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ApplyJsonServlet</servlet-name>
    <servlet-class>com.android.servlet.ApplyJsonServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ChangeApplyJsonServlet</servlet-name>
    <servlet-class>com.android.servlet.ChangeApplyJsonServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FindPropertyJsonServlet</servlet-name>
    <servlet-class>com.android.servlet.FindPropertyJsonServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>collectServlet</servlet-name>
    <servlet-class>com.android.servlet.collectServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CollectJsonServlet</servlet-name>
    <servlet-class>com.android.servlet.CollectJsonServlet</servlet-class>
  </servlet>











  <servlet-mapping>
    <servlet-name>UserJsonServlet</servlet-name>
    <url-pattern>/UserJsonServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginJsonServlet</servlet-name>
    <url-pattern>/LoginJsonServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FacilityJsonServlet</servlet-name>
    <url-pattern>/FacilityJsonServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PropertyJsonServlet</servlet-name>
    <url-pattern>/PropertyJsonServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CityJsonServlet</servlet-name>
    <url-pattern>/CityJsonServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UploadJsonServlet</servlet-name>
    <url-pattern>/UploadJsonServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ApplyJsonServlet</servlet-name>
    <url-pattern>/ApplyJsonServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ChangeApplyJsonServlet</servlet-name>
    <url-pattern>/ChangeApplyJsonServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FindPropertyJsonServlet</servlet-name>
    <url-pattern>/FindPropertyJsonServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>collectServlet</servlet-name>
    <url-pattern>/collectServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CollectJsonServlet</servlet-name>
    <url-pattern>/CollectJsonServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
